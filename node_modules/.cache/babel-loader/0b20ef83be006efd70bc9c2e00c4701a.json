 <div v-for=\"(pokemon, i) in pokemons\" :key=\"i\" class=\"col mb-4\">\n        <Pokemon :pokemon=\"pokemon\" :show-open-button=\"true\"/>\n      </div>\n    </div>\n\n    <nav aria-label=\"Page navigation example\">\n      <ul class=\"pagination justify-content-center\">\n        <li v-if=\"prev\" class=\"page-item\">\n          <button v-on:click=\"prevPage()\" class=\"page-link\" href=\"#\" tabindex=\"-1\">Previous</button>\n        </li>\n        <li class=\"page-item\">\n          <button v-on:click=\"nextPage()\" class=\"page-link\">Next</button>\n        </li>\n      </ul>\n    </nav>\n  </div></div>\n</template>\n\n<script>\nimport axios from \"axios\";\n\nexport default {\n  name: \"Home\",\n\n  data() {\n    return {\n      total: null,\n      pokemons: [],\n      next: '',\n      prev: Boolean,\n      current: 'https://pokeapi.co/api/v2/pokemon/?offset=0&limit=20'\n    };\n  },\n\n  methods: {\n    getPokemons () {\n      axios.get(this.current)\n        .then(res => {\n          this.pokemons = res.data.results;\n          this.next = res.data.next;\n          this.prev = res.data.previous;\n          this.total = res.data.count;\n        })\n        .catch(res => {\n          console.log(res);\n        });\n    },\n    nextPage () {\n      this.current = this.next;\n      this.getPokemons();\n    },\n    prevPage () {\n      this.current = this.prev;\n      this.getPokemons();\n    }\n  },\n  created () {\n    this.getPokemons();\n  }\n}\n</script>\n\n<style>\n.list-element {\n  margin-bottom: 10px;\n  border-bottom: 1px solid #dedede;\n}\n\n.float-right {\n  float: right;\n}\n\n.centered {\n  text-align: center;\n}\n</style>\n"],"sourceRoot":"src/views"}]}